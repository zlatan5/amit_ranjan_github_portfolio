{
  "name": "@sencha/ext",
  "version": "7.9.0",
  "sencha": {
    "name": "ext",
    "namespace": "Ext",
    "type": "framework",
    "creator": "Sencha",
    "summary": "Ext JS",
    "detailedDescription": "Sencha Ext JS JavaScript Framework",
    "version": "7.9.0",
    "compatVersion": "7.9.0",
    "format": "1",
    "output": "${package.dir}/build",
    "local": true,
    "sass": {
      "fashion": true,
      "namespace": "Ext",
      "etc": "${package.dir}/sass/etc/all.scss",
      "var": "${package.dir}/sass/var",
      "src": "${package.dir}/sass/src"
    },
    "classpath": "${package.dir}/src",
    "overrides": "${package.dir}/overrides",
    "resource": {
      "paths": [
        "resources"
      ]
    },
    "language": {
      "js": {
        "input": {
          "version": "ES5"
        }
      }
    },
    "properties": {
      "skip.sass": 1,
      "skip.slice": 1
    },
    "extTier": "pro"
  },
  "scripts": {
    "install": "node activate.js"
  },
  "license": "SEE LICENSE IN license.txt",
  "description": "Ext JS",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/extjs/SDK.git"
  },
  "dependencies": {
    "@sencha/ext-core": "7.9.0"
  },
  "author": "Sencha, Inc.",
  "homepage": "https://www.sencha.com/products/extjs/",
  "bugs": "https://support.sencha.com/",
  "keywords": [
    "sencha",
    "extjs",
    "ext js",
    "ext-js",
    "ext"
  ]
}